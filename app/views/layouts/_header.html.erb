<div class="row">
  <div class="col-sm-12">
<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "Webnish", root_path, id: "logo" %>
    <nav>



      <ul class="nav navbar-nav navbar-right">
        <li>
          <div class="hd">
            <%= form_tag search_results_path, method: 'get', class: 'search-form' do %>
              <div class="form-group has-feedback">
                <label for="search" class="sr-only">Search</label>
                  <%= text_field_tag :search_keywords, params[:search_keywords], placeholder: "Search", class: "form-control"%>

          <% end %>
                <%= button_tag do %>
                    <span class="glyphicon glyphicon-search form-control-feedback"></span>

            <% end %>

          </div></div>
            </li>

        <li><%= link_to "Home", root_path %></li>
        <li><%= link_to "Help", '#' %></li>

        <li><%= link_to 'Organzations',organizations_path %></li>

        <% if logged_in? %>
            <li><%= link_to "Users", '#' %></li>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                Account <b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><%= link_to "Profile", current_user %></li>
                <li><%= link_to "Settings", '#' %></li>
                <li class="divider"></li>
                <li>
                  <%= link_to "Log out", logout_path, method: "delete" %>
                </li>
              </ul>
            </li>
        <% else %>
            <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
    </div>
 </div>
